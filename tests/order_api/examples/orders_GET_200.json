{
    "partial": {
        "http-request": {
            "path": "/orders",
            "method": "GET",
            "query": {
                "status": "$(ENTITY.status)"
            },
            "headers": {
                "Authenticate": "API-TOKEN"
            }
        },
        "http-response": {
            "status": 200,
            "body": [
                {
                    "status": "$eq(REQUEST.QUERY-PARAMS.status)",
                    "id": "$array_has(ENTITY.id)",
                    "$if": {
                        "$conditions": {
                            "id": "$eq(ENTITY.id)"
                        },
                        "$then": {
                            "productId": "$(ENTITY.productId)",
                            "count": "$(ENTITY.count)",
                            "status": "$(ENTITY.status)"
                        }
                    }
                }
            ],
            "status-text": "OK",
            "headers": {
                "Content-Type": "application/json"
            }
        }
    }
}